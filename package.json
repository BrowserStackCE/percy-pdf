{
    "name": "percy-pdf",
    "version": "0.2.0",
    "description": "PDF Document Visual Automation Testing using Percy",
    "main": "tests/run.js",
    "scripts": {
        "stop-server": "forever stopall",
        "start-server": "forever start \"node_modules/http-server/bin/http-server\" \"-s\" \"-p 8080\" \"pdfjs-3.4.120-dist\"",
        "pretest": "ver && extract-zip %pwd%/pdfjs-3.4.120-dist.zip || extract-zip ./pdfjs-3.4.120-dist.zip; npm run start-server",
        "test": "node tests/run.js",
        "posttest": "npm run stop-server; exit 0;"
    },
    "keywords": [
        "percy-pdf",
        "pdf-doc",
        "visual-testing",
        "percy"
    ],
    "author": "samirans89",
    "license": "Apache License 2.0",
    "devDependencies": {
        "@percy/cli": "^1.26.2"
    },
    "dependencies": {
        "concurrently": "^8.2.0",
        "extract-zip": "^2.0.1",
        "forever": "^4.0.3",
        "fs": "0.0.1-security",
        "http-server": "^14.1.1",
        "js-yaml": "^4.1.0",
        "md5": "^2.3.0",
        "npm-run-all": "^4.1.5",
        "pdfjs-dist": "^3.7.107"
    }
}
